	pages = {211--227},
	file = {Abrutyn and Mueller - 2014 - Are Suicidal Behaviors Contagious in Adolescence .pdf:/Users/kkiley/Zotero/storage/XXI6GG4X/Abrutyn and Mueller - 2014 - Are Suicidal Behaviors Contagious in Adolescence .pdf:application/pdf},
}

