
@article{zuckerman2012,
	title = {Construction, {Concentration}, and ({Dis}){Continuities} in {Social} {Valuations}},
	volume = {38},
	url = {https://doi.org/10.1146/annurev-soc-070210-075241},
	doi = {10.1146/annurev-soc-070210-075241},
	abstract = {I review and integrate recent sociological research that makes progress on three interrelated questions pertaining to social valuation: (a) the degree of social construction relative to objective constraints; (b) the degree of concentration in social valuations at a single point in time; and (c) the conditions that govern two broad forms of temporal discontinuity—(i) fashion cycles, especially in cultural expression and in managerial practices, and (ii) bubble/crash dynamics, as witnessed in such domains as authoritarian regimes and financial markets. In the course of the review, I argue for the importance of identifying how objective conditions constrain social construction and suggest two contrarian mechanisms by which this is accomplished—valuation opportunism and valuation entrepreneurship—and the conditions under which they are more or less effective.},
	number = {1},
	urldate = {2020-04-28},
	journal = {Annual Review of Sociology},
	author = {Zuckerman, Ezra W.},
	year = {2012},
	note = {\_eprint: https://doi.org/10.1146/annurev-soc-070210-075241},
	keywords = {culture, fashion, social construction, valuation, financial markets},
	pages = {223--245},
	file = {Full Text PDF:/Users/kkiley/Zotero/storage/3E3P9UA6/Zuckerman - 2012 - Construction, Concentration, and (Dis)Continuities.pdf:application/pdf},
}

@article{baldassarri2007,
	title = {Dynamics of {Political} {Polarization}},
	volume = {72},
	issn = {0003-1224},
	url = {https://doi.org/10.1177/000312240707200507},
	doi = {10.1177/000312240707200507},
	abstract = {This article accounts for two puzzling paradoxes. The first paradox is the simultaneous absence and presence of attitude polarization?the fact that global attitude polarization is relatively rare, even though pundits describe it as common. The second paradox is the simultaneous presence and absence of social polarization?the fact that while individuals experience attitude homogeneity in their interpersonal networks, their networks are characterized by attitude heterogeneity. These paradoxes give rise to numerous scholarly arguments. By developing a formal model of interpersonal influence over attitudes in a context where individuals hold simultaneous positions on multiple issues, we show why these arguments are not mutually exclusive and how they meaningfully refer to the same social setting. The results from this model provide a single parsimonious account for both paradoxes. The framework we develop may be generalized to a wider array of problems, including classic problems in collective action.},
	number = {5},
	urldate = {2020-04-28},
	journal = {American Sociological Review},
	author = {Baldassarri, Delia and Bearman, Peter},
	month = oct,
	year = {2007},
	note = {Publisher: SAGE Publications Inc},
	keywords = {attitudes, polarization, computational social science, formal models, agent-based models, attitude polarization, political polarization, global polarization, social polarization, homogeneity, heterogeneity, interpersonal influence, collective action, simulation methods},
	pages = {784--811},
	file = {Baldassarri and Bearman - 2007 - Dynamics of Political Polarization.pdf:/Users/kkiley/Zotero/storage/ZUFJSPRU/Baldassarri and Bearman - 2007 - Dynamics of Political Polarization.pdf:application/pdf},
}

@article{baldassarri2008,
	title = {Partisans without {Constraint}: {Political} {Polarization} and {Trends} in {American} {Public} {Opinion}},
	volume = {114},
	abstract = {Public opinion polarization is here conceived as a process of alignment along multiple lines of potential disagreement and measured as growing constraint in individuals’ preferences. Using NES data from 1972 to 2004, the authors model trends in issue partisanship—the correlation of issue attitudes with party identification—and issue alignment—the correlation between pairs of issues—and find a substantive increase in issue partisanship, but little evidence of issue alignment. The findings suggest that opinion changes correspond more to a resorting of party labels among voters than to greater constraint on issue attitudes: since parties are more polarized, they are now better at sorting individuals along ideological lines. Levels of constraint vary across population subgroups: strong partisans and wealthier and politically sophisticated voters have grown more coherent in their beliefs. The authors discuss the consequences of partisan realignment and group sorting on the political process and potential deviations from the classic pluralistic account of American
politics.},
	language = {en},
	number = {2},
	journal = {American Journal of Sociology},
	author = {Baldassarri, Delia and Gelman, Andrew},
	year = {2008},
	keywords = {attitudes, ideology, polarization, public opinion, constraint, political ideology, political attitudes, belief formation, belief structure, political identity, political polarization, partisanship, party identification, opinion change},
