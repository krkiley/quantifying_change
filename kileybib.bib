	pages = {569--586},
	file = {Full Text PDF:/Users/kkiley/Zotero/storage/N5K2JXSR/Orr and Huber - 2020 - The Policy Basis of Measured Partisan Animosity in.pdf:application/pdf;Snapshot:/Users/kkiley/Zotero/storage/EGSRSKGU/ajps.html:text/html},
}

@article{halpern-manners2017,
