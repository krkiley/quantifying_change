	author = {Schilke, Oliver and Rossman, Gabriel},
	month = dec,
	year = {2018},
	note = {Publisher: SAGE Publications Inc},
	keywords = {bribery, economic sociology, legitimacy, obfuscation, vignette experiments, morality},
