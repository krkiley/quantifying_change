	volume = {100},
	issn = {0090-0036, 1541-0048},
	shorttitle = {The {Impact} of {Institutional} {Discrimination} on {Psychiatric} {Disorders} in {Lesbian}, {Gay}, and {Bisexual} {Populations}},
