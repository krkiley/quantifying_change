@phdthesis{leifer1983,
	address = {United States -- Massachusetts},
	type = {Ph.{D}.},
	title = {Robust {Action}: {Generating} {Joint} {Outcomes} in {Social} {Relationships}},
	copyright = {Database copyright ProQuest LLC; ProQuest does not claim copyright in the individual underlying works.},
	shorttitle = {Robust {Action}},
	url = {http://www.proquest.com/pqdtglobal/docview/303277869/abstract/5706F85326F34862PQ/22},
	abstract = {The topic is rational social action in social relationships predicated on "equality." A rationality suitable for the context of sustained equality is developed. The rationality is built on self-limiting behavior and permits both outcomes and their evaluation to be jointly determined through the interaction process. This rationality is pitted against the ex ante rationality of game theory, which treats possible outcomes and their evaluation as "givens," fixed prior to the onset of interaction. In game theory "equality" is not well-defined. Equality becomes something "real" only in stepping outside the actor's viewpoint in looking at action sequences from the standpoint of the relationship. Self-limiting behavior makes equality possible.
"Class" tournaments in chess served as the empirical setting, where players matched on skill were paired. High skilled players should best approximate the predictions of a valid theory of rational social action. In balanced positions high skilled players were found to be (1) the least likely to arrive at outcomes on the basis of initial strategies; (2) the most likely to exert decision activity where resulting choices least constrained outcomes; and (3) the least likely to attain outcomes that were interpretable within the one-sided objectives of chess. These findings are consistent with the proposed rationality, as it strips outcomes of any ex ante basis. A notion of "robust" action is offered to account for the sequentially organized pursuit of unforeseen "subtle" outcomes by high skilled players.
High skilled players became the most "intentional" in their outcome pursuit when balance broke down, arriving at outcomes on the basis of initial strategies. This "contextual" effect is central in the proposed theory. The "context" of relative control is the backdrop for assessing the rationality of decision processes and action, rather than the "context" of stable, joint utility distributions. Utility judgments must be flexible in symmetric relationships to arrive at joint evaluations. "Equality" itself becomes a "context" for rational social action. The issue of relationship failures is explored by examining what happens when either rationality or equality breaks down.},
	language = {English},
	urldate = {2021-05-27},
	school = {Harvard University},
	author = {Leifer, Eric M.},
	year = {1983},
	note = {ISBN: 9798643140375},
	keywords = {Social sciences},
	file = {Full Text PDF:/Users/kkiley/Zotero/storage/RK2TWBEW/LEIFER - Robust Action Generating Joint Outcomes in Social.pdf:application/pdf},
}

@article{oberschall1986,
	title = {Efficiency and {Social} {Institutions}: {Uses} and {Misuses} of {Economic} {Reasoning} in {Sociology}},
	volume = {12},
	abstract = {We review recent applications of the "new institutional economics" to a variety of social institutions. The applications use the idea of efficiency to account for the emergence and persistence of institutions such as the family, sharing groups, private property, discrimination, and the norm of reciprocity. Efficien­ cy entails eliminating costly externalities with the least possible transaction costs (i.e. costs involved in negotiating, writing, and enforcing agreements). Our critique of efficiency shows how power relations, goal ambiguity. and the institutional relativism of choice render efficiency problematic. The sociologi­ cal criterion of reproducibility may be more relevant where these features hold. If efficiency analysis is used, the sociologist should insist that it allow the identification of inefficiencies and that institutional participants welcome sug­ gested improvements in efficiency.},
