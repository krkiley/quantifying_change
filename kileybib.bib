	file = {McPherson et al. - 2001 - Birds of a Feather Homophily in Social Networks.pdf:/Users/kkiley/Zotero/storage/5Z85XF42/McPherson et al. - 2001 - Birds of a Feather Homophily in Social Networks.pdf:application/pdf},
}

@article{martin2000,
