	keywords = {attitudes, attitude change, attitude strength, social networks, ambivalence, attitude stability, congruent networks, heterogeneous networks},
	pages = {779--795},
	file = {Citeseer - Full Text PDF:/Users/kkiley/Zotero/storage/87EYYI57/Visser and Mirabile - 2004 - Attitudes in the social context The impact of soc.pdf:application/pdf;Citeseer - Snapshot:/Users/kkiley/Zotero/storage/7G86MWLI/summary.html:text/html},
}

